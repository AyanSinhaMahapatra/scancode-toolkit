  "\t \r\n",
  "\t \r\n",
  "\t\t Startup date \r\n",
  "\t\t <format:formatDate value=\"<%= serverInfos.getStartupTime() %>\" type=\"both\" pattern=\"yyyy-MM-dd HH:mm:ss\"/> \r\n",
