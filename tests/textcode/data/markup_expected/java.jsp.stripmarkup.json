  " <core:out value='<%= request.getAttribute(\"message\") %>'/> \r\n",
  "\r\n",
  " \r\n",
  "<core:forEach items=\"<%= serverInfos.getServerSpecificData() %>\" var=\"serverSpecificData\" varStatus=\"status\">\r\n",
